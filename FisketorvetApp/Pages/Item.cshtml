@page
@model FisketorvetApp.Pages.ItemModel
@{
}

<!-- Ashanti -->

<div class="container">
    <h1 class="text-center">Name of store</h1>
    <div class="row text-center">
        <div class="col-md-6">
            <img class="" src="/wp-content/themes/creativeforces/images/majapic.jpg" width="250">
            <h3 class="text-center">Name store</h3>
            <h3 class="text-center">Open from 2 to 3</h3>
        </div>
        <div class="col-md-6">
            <p><strong>Store name</strong> Description.</p>
        </div>
    </div>
</div>

    <table class="table">
        <thead>
            <tr>
                <th scope="col">ID</th>
                <th scope="col">Name</th>
                <th scope="col">Price</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @if (Model.ListOfItems != null)
            {
                foreach (var item in Model.ListOfItems)
                {
                    <tr>
                        <td>
                            @item.Id
                        </td>
                        <td>
                            @item.Name
                        </td>
                        <td>
                            @item.Price
                        </td>
                        <td>
                        </td>
                    </tr>
                }
            }
        </tbody>
    </table>
